
#
# Database Connection Parameters
#
hibernate.dialect=org.hibernate.dialect.MySQLDialect
javax.persistence.jdbc.driver=com.mysql.jdbc.Driver
# For testing use tsdbtest
javax.persistence.jdbc.url=jdbc:mysql://127.0.0.1:3306/tsdb
javax.persistence.jdbc.user=root
javax.persistence.jdbc.password=EDIT_THIS

#
# JPA settings
#
hibernate.show_sql=false
hibernate.format_sql=true
hibernate.use_sql_comments=true
hibernate.jdbc.batch_size=500
hibernate.jdbc.default_batch_fetch_size=500

#
# Lucene index base
#
hibernate.search.default.indexBase=EDIT_THIS/indexes
hibernate.search.lucene_version=LUCENE_36
hibernate.search.default.directory_provider=filesystem

#
# Metadata Service Settings (one for each "terminology")
#
metadata.service.handler=SNOMEDCT,ICD9CM
metadata.service.handler.SNOMEDCT.class=org.ihtsdo.otf.ts.jpa.services.helper.SnomedMetadataServiceJpaHelper
metadata.service.handler.ICD9CM.class=org.ihtsdo.otf.ts.jpa.services.helper.ClamlMetadataServiceJpaHelper

#
# Validation Service Settings
#
validation.service.handler=NEW_CONCEPT
validation.service.handler.NEW_CONCEPT.class=org.ihtsdo.otf.ts.services.validation.NewConceptMinRequirementsCheck
validation.service.handler.NEW_CONCEPT.isaRel=116680003
validation.service.handler.NEW_CONCEPT.fnType=900000000000003001
validation.service.handler.NEW_CONCEPT.ptType=900000000000013009
validation.service.handler.NEW_CONCEPT.preferredId=900000000000548007
validation.service.handler.NEW_CONCEPT.languageId=900000000000509007

#
# workflow.service.handler
#
workflow.listener.handler=DEFAULT
workflow.listener.handler.DEFAULT.class=org.ihtsdo.otf.ts.jpa.services.handlers.DefaultWorkflowListener

#
# Graph Service Settings
#
graph.resolution.handler=DEFAULT
graph.resolution.handler.DEFAULT.class=org.ihtsdo.otf.ts.services.handlers.DefaultGraphResolutionHandler

#
# SNOMEDCT data location
#
loader.SNOMEDCT.input.data=C:/data/snomedct-20140731-mini
loader.SNOMEDCT.delta.data=C:/data/snomed-dailybuild-mini
# SNOMED CT RF2 SCTID for "Synonym"
loader.defaultPreferredNames.typeId=900000000000013009
# SNOMED CT RF2 SCTID for the US English language refset
loader.defaultPreferredNames.refSetId=900000000000509007
# SNOMED CT RF2 SCTID for "Preferred"
loader.defaultPreferredNames.acceptabilityId=900000000000548007

#
# ICD9CM data location
#
loader.ICD9CM.input.data=C:/data/icd9cm-2013.xml

#
# Webapp base URL (currently unused)
#
base.url=

#
# URL for security service
#
security.timeout=7200000
security.handler=DEFAULT
security.handler.DEFAULT.class=org.ihtsdo.otf.ts.services.handlers.DefaultSecurityServiceHandler
#security.handler.IHTSDO.class=org.ihtsdo.otf.ts.services.handlers.IHTSDOSecurityServiceHandler
#security.handler.IHTSDO.url=https://usermanagement.ihtsdotools.org/security-web/query/

