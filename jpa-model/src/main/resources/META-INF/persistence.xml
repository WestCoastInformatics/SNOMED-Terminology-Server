<?xml version="1.0" encoding="UTF-8"?>

<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <!-- use transaction-type RESOURCE_LOCAL because we do not have a container -->
  <persistence-unit name="TermServiceDS" transaction-type="RESOURCE_LOCAL">

    <!-- JPA persistence provider. -->
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <!-- List managed classes -->
    <class>org.ihtsdo.otf.ts.rf2.jpa.AbstractComponent</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.ConceptJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.DescriptionTypeRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.DescriptionJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.RelationshipJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.AbstractRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.AbstractConceptRefSetMember</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.AbstractDescriptionRefSetMember</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.AttributeValueConceptRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.ComplexMapRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.LanguageRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.ModuleDependencyRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.RefsetDescriptorRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.SimpleMapRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.SimpleRefSetMemberJpa</class>
    <class>org.ihtsdo.otf.ts.rf2.jpa.TransitiveRelationshipJpa</class>
    <class>org.ihtsdo.otf.ts.jpa.UserJpa</class>
  </persistence-unit>
</persistence>
